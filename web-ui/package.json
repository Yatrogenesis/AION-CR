  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "preview": "vite preview",
    "serve": "serve -s dist -l 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:ci": "start-server-and-test dev http://localhost:5173 'cypress run'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --project-token=$CHROMATIC_PROJECT_TOKEN",
    "size": "size-limit",
    "analyze": "npm run build:analyze && npx webpack-bundle-analyzer dist/assets/*.js",
    "clean": "rimraf dist coverage .nyc_output storybook-static",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "docker:build": "docker build -t aion-cr-web-ui .",
    "docker:run": "docker run -p 3000:3000 aion-cr-web-ui",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:audit": "npm audit",
    "deps:audit-fix": "npm audit fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "1 MB"
    },
    {
      "path": "dist/assets/*.css",
      "limit": "100 KB"
    }
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.1.0"
  }
}